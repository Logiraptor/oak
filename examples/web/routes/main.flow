frontend web();

component eq Eq();
component cond Cond();
component responder HTTPResponder();
component request HTTPRequest();
component path FieldAccessor("Path");

send "Goodbye!" to cond.Consequence;
send "Hello!" to cond.Alternative;
send "/bye" to eq.RHS;
send 200 to responder.Status;

connect request.Output to path.Input;
connect path.Output to eq.LHS;
connect eq.Output to cond.Condition;
connect cond.Output to responder.Body;
